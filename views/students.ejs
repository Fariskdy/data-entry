<%- include('./layouts/header') %>

<section data-bs-version="5.1" class="features035 table-top" id="features035-s">




    <div class="container">
        <div class="row">
            <div class="col-12 col-lg-9 active">
                <div class="item-wrapper">
                    <div class="card-box">
                        <h4 class="card-text mbr-fonts-style display-5"><strong>Students List</strong></h4>
                    </div>
                </div>
            </div>
            <div class="col-12 col-lg-3 active">
                <div class="mbr-section-btn"><a class="btn btn-primary display-7" href="/admin/dirasa/download-student-list">Download
                        Excel&nbsp;</a></div>
            </div>
        </div>
    </div>
</section>

<section data-bs-version="5.1" class="section-table table-bottom" id="design-block-xohF2Flblq">
    <div class="container container-table">
        <div class="table-wrapper">
            <div class="container">
                <div class="row search">
                    <div class="col-md-6"></div>
                    <div class="col-md-6">
                        <div class="dataTables_filter"> <label
                                class="searchInfo mbr-fonts-style display-7">Search:</label> <input
                                class="form-control input-sm" disabled=""> </div>
                    </div>
                </div>
            </div>
            <div class="container scroll">
                <table class="table isSearch" cellspacing="0">
                    <thead>
                        <tr class="table-heads ">
                            <th class="head-item mbr-fonts-style display-7" data-sortable="false">Teacher Name</th>
                            <th class="head-item mbr-fonts-style display-7" data-sortable="false">Student Name</th>
                            <th class="head-item mbr-fonts-style display-7" data-sortable="false">Enrollment Number</th>
                            <th class="head-item mbr-fonts-style display-7" data-sortable="false">Level</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% tableData.forEach(student => { %>
                            <tr>
                                <td class="body-item mbr-fonts-style display-7"><%= student.teacherName %></td>
                                <td class="body-item mbr-fonts-style display-7"><%= student.studentName %></td>
                                <td class="body-item mbr-fonts-style display-7"><%= student.enrollmentNumber %></td>
                                <td class="body-item mbr-fonts-style display-7"><%= student.studentLevel %></td>
                            </tr>
                        <% }); %>
                    </tbody>
                </table>
            </div>
            <div class="container table-info-container">
                <div class="row info">
                    <div class="col-md-6">
                        <div class="dataTables_info mbr-fonts-style display-7"> <span
                                class="infoBefore">Showing</span> <span class="inactive infoRows"></span> <span
                                class="infoAfter">entries</span> <span class="infoFilteredBefore">(filtered
                                from</span> <span class="inactive infoRows"></span> <span class="infoFilteredAfter">
                                total entries)</span> </div>
                    </div>
                    <div class="col-md-6"></div>
                </div>
            </div>
        </div>
    </div>
</section>








<%- include('./layouts/footer') %>